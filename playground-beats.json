{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "landingPage": "/beats-playground/",
  "preferredVersions": {
    "php": "8.2",
    "wp": "6.5"
  },
  "steps": [
    {
      "step": "log",
      "message": "Blueprint started",
      "capture": true
    },
    {
      "step": "log",
      "message": "Downloading plugin ZIP",
      "capture": true
    },
    {
      "step": "installPlugin",
      "pluginZipUrl": "https://codeload.github.com/crystalthedeveloper/wordpress-plugin-beats-upload-player/zip/refs/heads/main",
      "capture": true
    },
    {
      "step": "log",
      "message": "Plugin ZIP installed",
      "capture": true
    },
    {
      "step": "log",
      "message": "Activating plugin",
      "capture": true
    },
    {
      "step": "activatePlugin",
      "plugin": "wordpress-plugin-beats-upload-player-main/beats-upload-player/beats-upload-player.php",
      "capture": true
    },
    {
      "step": "log",
      "message": "Plugin activated",
      "capture": true
    },
    {
      "step": "log",
      "message": "Creating Beats Playground page",
      "capture": true
    },
    {
      "step": "createPost",
      "title": "Beats Playground",
      "slug": "beats-playground",
      "content": "<!-- wp:shortcode -->[beats_player_demo]<!-- /wp:shortcode -->",
      "capture": true
    },
    {
      "step": "log",
      "message": "Page created, setting as homepage",
      "capture": true
    },
    {
      "step": "setHomepage",
      "pageId": "{{posts.beats-playground}}",
      "capture": true
    },
    {
      "step": "log",
      "message": "Blueprint finished",
      "capture": true
    }
  ]
}
